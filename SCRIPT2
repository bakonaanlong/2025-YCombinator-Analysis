

SELECT
  *
FROM
  "PROJECT_YC"."PUBLIC"."YC_TABLE2"
;
 
ALTER TABLE YC_TABLE2
RENAME COLUMN C6 TO `AI?`;

ALTER TABLE YC_TABLE2
DROP ONELINER;

DELETE FROM YC_TABLE2
WHERE INDUSTRY = 'INDUSTRY';

UPDATE YC_TABLE2
SET SUBINDUSTRY = SUBSTRING(SUBINDUSTRY, 3, LEN(SUBINDUSTRY) - 4);

CREATE TABLE YC_TABLE3 AS
(SELECT *,
    SPLIT(SUBINDUSTRY, ',')[0]::STRING AS SUBINDUSTRY1,
    SPLIT(SUBINDUSTRY, ',')[1]::STRING AS SUBINDUSTRY2,
    SPLIT(SUBINDUSTRY, ',')[2]::STRING AS SUBINDUSTRY3
FROM YC_TABLE2);

SELECT * FROM YC_TABLE3;

ALTER TABLE YC_TABLE3
DROP SUBINDUSTRY, SUBINDUSTRY2;

ALTER TABLE YC_TABLE3
DROP SUBINDUSTRY3;

SELECT * FROM YC_TABLE3;

UPDATE YC_TABLE3
SET SUBINDUSTRY1 = REGEXP_REPLACE(SUBINDUSTRY1, '[[:punct:]]', '');

